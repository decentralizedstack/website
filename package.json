{
  "name": "dstack-website",
  "version": "0.0.1",
  "description": "DStack: A Decentralized Open Technology Stack",
  "main": "server.js",
  "scripts": {
    "compile": "./node_modules/.bin/harp compile src dist",
    "dev": "npm start & npm run watch",
    "shipit": "npm test && git push origin head",
    "start": "./node_modules/.bin/harp server --port ${PORT:=9000} src",
    "test": "npm run compile",
    "watch": "browser-sync start --port 8080 --proxy 'localhost:9000' --files 'src/**/*'"
  },
  "author": "Harlan T Wood",
  "license": "",
  "devDependencies": {
    "browser-sync": "^1.3.7"
  },
  "dependencies": {
    "harp": "^0.13.0"
  },
  "engines": {
    "node": "~0.10.38",
    "npm": "^2.7.6"
  }
}

